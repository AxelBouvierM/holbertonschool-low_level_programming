#include "main.h"
/**
 *rev_string - Prints reverse string.
 *@s: First array
 *@_strlen
 */
void rev_string(char *s)
{
	int i;
	int j = _strlen - 1;
	char str[];

	for (i = 0; i < j; i++);

{
	str[0] = s[j];
	s[j] = s[i];
	s[i] = str[0];
	j--;
}
}

/**
 *_strlen - Prints out a string to 10.
 *@s: First variable
 * Return: count
 */
int _strlen(char *s)
{
	int count = 0;

	while (*s != '\0')
{
	count++;
	s++;
}
	return (count);
}
