#include "main.h"

/**
 * times_table - check the code
 *
 * Return: Always 0.
 */
void times_table(void)
{
int n;
int m;
int result;

for (n = 0 ; n <= 10 ; n++)
{
for (m = 0 ; <= 10 ; m++)
{
result = n * m;
}
if (result >= 10)
{
_putchar(result / 10 + '0');
_putchar(result % 10 + '0');
}
else if (result < 10)
{
_putchar(' ');
_putchar(result % 10 + '0');
}
}
}
